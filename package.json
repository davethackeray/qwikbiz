{
  "name": "qwikbiz",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "verify-env": "NODE_OPTIONS='--loader ts-node/esm' ts-node --esm scripts/verify-env.ts",
    "predev": "npm run verify-env",
    "dev": "next dev --turbopack",
    "prebuild": "npm run verify-env && node scripts/prebuild.js",
    "build": "next build",
    "prestart": "npm run verify-env && node scripts/prebuild.js",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:ci": "npm run verify-env && jest",
    "test:performance": "node --experimental-specifier-resolution=node --expose-gc dist/performance/src/features/simulation/__tests__/performance/runner.js",
    "test:performance:watch": "nodemon --experimental-specifier-resolution=node --exec node --expose-gc dist/performance/src/features/simulation/__tests__/performance/runner.js",
    "pretest:performance": "rimraf dist/performance && tsc -p tsconfig.perf.json"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.2.1",
    "@testing-library/jest-dom": "^6.6.3",
    "axios": "^1.7.9",
    "chart.js": "^4.4.7",
    "dotenv": "^16.4.7",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jsonwebtoken": "^9.0.2",
    "next": "15.1.6",
    "next-auth": "^4.24.11",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "postcss": "^8",
    "prisma": "^6.2.1",
    "ps-node": "^0.1.6",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
